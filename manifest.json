{
    "manifest_version": 3,
    "name": "Short-Link",
    "version": "1.0",
    "permissions": [
      "scripting",
      "activeTab",
      "commands"
    ],
    "host_permissions": [
      "<all_urls>"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "commands": {
      "send-page-html": {
        "suggested_key": {
          "default": "Ctrl+Q"
        },
        "description": "Send page HTML to Python server"
      }
    }
  }
  